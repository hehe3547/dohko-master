<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:dohko="http://www.dohko.cn/schema/dohko"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
	http://www.dohko.cn/schema/dohko http://www.dohko.cn/schema/dohko.xsd">

	<dohko:filter id="/aa/acc.htm" service="my_simpleService" req-data="my_dataset" res-data="my_dataset"/>

	<dohko:dataset id="my_dataset">
		<dohko:props>
			<dohko:data key="kaaa"/>
			<dohko:data key="a"/>
			<dohko:data key="b"/>
			<dohko:data key="c"/>
			<dohko:data key="d"/>
		</dohko:props>
		<dohko:records>
			<dohko:data key="kaaa"></dohko:data>
		</dohko:records>
		<dohko:records key="foodList">
			<dohko:data key="kaaa"></dohko:data>
			<dohko:data key="ka1a"></dohko:data>
		</dohko:records>
